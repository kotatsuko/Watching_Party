<% if end_user != current_end_user %>
  <% if current_end_user.following?(end_user) %>
    <%= link_to "フォローを外す", end_user_relationships_path(end_user.id),class:"btn btn-info", method: :delete, remote:true %>
  <% else %>
    <%= link_to "フォローする", end_user_relationships_path(end_user.id),class:"btn btn-success", method: :post, remote:true %>
  <% end %>
<% end %>